{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "Node",
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@store/*": ["store/*"]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
